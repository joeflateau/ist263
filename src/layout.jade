mixin slide(x, y, z, rotX, rotY, rotZ)
  div(data-x=x,data-y=y,data-z=z,data-rotate-x=rotX,data-rotate-y=rotY,data-rotate-z=rotZ,class=(attributes.class||"") + " step")
    block

mixin poll(id, options)
  div.poll(id=id)
    h4 You respond:
    ul
      each val in options
        li= val
    svg


doctype html
html(lang="en")
  head
